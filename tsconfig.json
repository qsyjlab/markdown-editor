{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "module": "esnext",
    "baseUrl": ".",
    "sourceMap": false,
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,

    "esModuleInterop": true,
    "removeComments": false,
    "skipLibCheck": true,
    "rootDir": "./",
    "types": [],
    "paths": {
      "md-doc-editor/*": [
        "packages/*"
      ]
    }
  },
}
